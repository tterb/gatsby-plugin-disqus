{
  "name": "gatsby-plugin-disqus-site",
  "version": "1.0.0",
  "description": "A website to showcase gatbsy-plugin-disqus",
  "main": "index.js",
  "scripts": {
    "build": "cleandir ./.cache && gatsby build",
    "develop": "cleandir ./.cache && gatsby develop",
    "start": "npm run develop",
    "lint": "eslint ./src",
    "setup": "node ./bin/setup.js",
    "deploy": "gatsby build --prefix-paths"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tterb/gatsby-plugin-disqus.git"
  },
  "keywords": [
    "gatsby",
    "plugin",
    "disqus"
  ],
  "author": "Brett Stevenson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tterb/gatsby-plugin-disqus/issues"
  },
  "homepage": "https://github.com/tterb/gatsby-plugin-disqus#readme",
  "dependencies": {
    "@mdx-js/mdx": "^1.0.19",
    "@mdx-js/react": "^1.0.16",
    "babel-plugin-styled-components": "^1.10.0",
    "clean-dir": "^1.0.3",
    "dotenv": "^7.0.0",
    "envfile": "^3.0.0",
    "gatsby": "^2.3.27",
    "gatsby-image": "^2.0.41",
    "gatsby-mdx": "^0.6.3",
    "gatsby-plugin-disqus": "^1.0.3",
    "gatsby-plugin-google-analytics": "^2.0.18",
    "gatsby-plugin-manifest": "^2.0.29",
    "gatsby-plugin-netlify": "^2.1.22",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-sharp": "^2.0.30",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-remark-emojis": "^0.3.1",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^3.0.11",
    "gatsby-remark-prismjs": "^3.2.4",
    "gatsby-remark-responsive-iframe": "^2.1.1",
    "gatsby-source-filesystem": "^2.0.38",
    "gatsby-transformer-remark": "^2.3.10",
    "gatsby-transformer-sharp": "^2.1.17",
    "inquirer": "^6.2.0",
    "node-sass": "^4.11.0",
    "prismjs": "^1.15.0",
    "prop-types": "^15.6.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-fontawesome": "^1.6.1",
    "react-headroom": "^2.2.4",
    "react-helmet": "^5.2.0",
    "react-markdown": "^4.0.8",
    "react-reveal": "^1.2.2",
    "react-scroll-section": "^1.3.0",
    "react-tippy": "^1.2.2",
    "rebass": "^3.0.0",
    "styled-components": "^4.0.2",
    "styled-icons": "^7.15.1",
    "tailwind.macro": "^0.5.10",
    "tailwindcss": "^0.7.3",
    "typeface-poppins": "^0.0.72",
    "typeface-source-code-pro": "0.0.71",
    "typeface-titillium-web": "^0.0.54"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "chalk": "^2.4.1",
    "contentful-import": "^7.4.0",
    "eslint": "^5.11.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-react": "^7.17.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
